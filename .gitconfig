[user]
    name = yashigani
    email = tai.fukui@gmail.com
[core]
    pager = lv -c
    editor = vim
    excludesfile = /Users/taiki/.gitignore
    autocrlf = input
	quotepath = false
[color]
    ui = auto
[alias]
    st = status
    sh = show
    br = branch
    co = checkout
    cb = checkout -b
    cm = commit
    ca = commit -a
    gr = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short
# for git-svn
    srb = svn rebase
    scm = svn dcommit
